{
    "name": "tobya/laravel-mobile-agent",
    "description": "Desktop/mobile user agent parser with support for Laravel, based on Mobiledetect, based on jenssegers/agent and /Bogddan/agent",
    "keywords": ["laravel", "useragent", "agent", "user agent", "browser", "platform", "mobile", "desktop"],
    "homepage": "https://github.com/Bogddan/agent",
    "license" : "MIT",
    "authors": [
        {
            "name": "Jens Segers",
            "homepage": "https://Bogddan.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "mobiledetect/mobiledetectlib": "^3.74",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "php-coveralls/php-coveralls": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Bogddan\\Agent\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Bogddan\\Agent\\AgentServiceProvider"
            ],
            "aliases": {
                "Agent": "Bogddan\\Agent\\Facades\\Agent"
            }
        }
    },
    "suggest": {
        "illuminate/support": "Required for laravel service providers"
    }
}
